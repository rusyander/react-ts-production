{
    "name": "react-ts-production",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "i": "npm i --force",
        "build": "webpack --config webpack.config.ts",
        "server": "node ./json-server/index.js",
        "dev": "webpack serve --env port=9090",
        "start": "concurrently \"npm run dev\" \"npm run server\" ",
        "start:vite": "concurrently \"vite --host 0.0.0.0 --port 9090\" \"npm run server\" ",
        "build:prod": "webpack --env mode=production apiUrl=production-server-seven.vercel.app",
        "build:dev": "webpack --env mode=development",
        "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
        "lint:ts:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
        "lint:css": "npx stylelint \"**/*.scss\"",
        "lint:css:fix": "npx stylelint \"**/*.scss\" --fix",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "storybook:build": "storybook build -c ./config/storybook",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:e2e": "cypress open",
        "test:ui": "npx loki test",
        "test:ui:ci:full": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" \"deelay\"",
        "test:ui:ok": "npx loki approve",
        "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "open": ".loki\\report.html",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html && npm run open",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "fix-imports": "ts-node ./scripts/refactoring/updateImports.ts",
        "create-indexFileForShared": "ts-node ./scripts/refactoring/createPublicApiFotSharedUi.ts",
        "postinstall": "node scripts/postinstall.js",
        "prettier": "npx prettier --write",
        "remove-feature": "node scripts/remove-feature.ts"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": "eslint"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.21.8",
